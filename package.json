{
  "name": "fiction-expo-social-auth",
  "version": "2.2.0",
  "description": "Effortless social authentication for React, React Native (Expo), Vue.js, Svelte, and Next.js. Supports Google, Facebook, and Apple Sign-In with a unified API.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./vue": {
      "import": "./dist/vue/index.js",
      "require": "./dist/vue/index.js",
      "types": "./dist/vue/index.d.ts"
    },
    "./svelte": {
      "import": "./dist/svelte/index.js",
      "require": "./dist/svelte/index.js",
      "types": "./dist/svelte/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "index.js"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "echo \"Tests not yet configured\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fictiondevelopers/fiction-expo-social-auth.git"
  },
  "keywords": [
    "social-media-login-provider",
    "effortless-social-logins",
    "expo",
    "react-native",
    "expo-auth",
    "google-signin",
    "apple-signin",
    "facebook-signin",
    "oauth",
    "nextjs",
    "react",
    "vue",
    "vue3",
    "svelte",
    "social-auth",
    "universal"
  ],
  "author": "Fiction Developers",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fictiondevelopers/fiction-expo-social-auth/issues"
  },
  "homepage": "https://github.com/fictiondevelopers/fiction-expo-social-auth#readme",
  "peerDependencies": {
    "expo-apple-authentication": ">=6.0.0",
    "expo-linking": ">=5.0.0",
    "expo-web-browser": ">=12.0.0",
    "react": ">=17.0.0",
    "vue": ">=3.0.0",
    "svelte": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "expo-apple-authentication": {
      "optional": true
    },
    "expo-linking": {
      "optional": true
    },
    "expo-web-browser": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "svelte": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/node": "^25.0.10",
    "@types/react": "^18.2.0",
    "svelte": "^4.0.0",
    "typescript": "^5.0.0",
    "vue": "^3.3.0"
  }
}